  "version": "0.2.0",
  "configurations": [

    {
        "name": "C++ (gdb) Launch",
        "preLaunchTask": "C/C++: g++ build active file",
        "type": "cppdbg",
        "request": "launch",
        "program": "${fileDirname}/${fileBasenameNoExtension}",
        "args": [],
        "stopAtEntry": true,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
    },

    {
        "name": "Python: Current File (Integrated Terminal)",
        "type": "python",
        "request": "launch",
        "cwd": "${fileDirname}",
        "args": [],
        "program": "${file}",
        "stopOnEntry": true,
        "console": "integratedTerminal"
      },
    {
        "name": "C (gdb) Launch",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceRoot}/a.out",
        "cwd": "${workspaceFolder}",
        "stopAtEntry": true,
        "MIMode": "gdb",
    },
